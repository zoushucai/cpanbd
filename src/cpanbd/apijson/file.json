			"web": "0: int: optional" // 默认为0,  为1时返回缩略图地址
		},
		"schema_": {
			"type": "object",
			"properties": {
				"info": {
					"type": "array",
					"items": {
						"type": "object",
						"properties": {
							"server_filename": {
								"type": "string"
							},
							"category": {
								"type": "number"
							},
							"fs_id": {
								"type": "number"
							},
							"size": {
								"type": "number"
							},
							"isdir": {
								"type": "number"
							},
							"path": {
								"type": "string"
							}
						},
						"required": ["server_filename", "category", "fs_id", "size", "isdir", "path"]
					}
				}
			}
		}
	},
